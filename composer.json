{
    "name": "watson/sitemap",
    "description": "Generate Google Sitemaps in Laravel 4/5",
    "keywords": ["laravel", "sitemaps"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@studious.com.au"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/console": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src/Watson"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Sitemap\\SitemapServiceProvider"
            ]
        }
    }
}
